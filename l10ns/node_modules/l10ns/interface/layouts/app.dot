
<div class="app" data-layout="app">
  <header class="app-header">
    <div class="app-left-header" data-region="leftHeader">
      {{=it.leftHeader}}
    </div>
    <div class="app-search" data-region="search">{{=it.search}}</div>
    <div class="app-locales" data-region="locales">{{=it.locales}}</div>
  </header>
  <div class="app-localizations {{?it.localization}}is-hidden{{?}}" {{?it.localization}}style="display:none;"{{?}} data-region="body">{{=it.body}}</div>
  <div class="app-localization {{?!it.localization}}is-hidden{{?}}" {{?!it.localization}}style="display:none;"{{?}} data-region="localization">{{?it.localization}}{{=it.localization}}{{?}}</div>
</div>
