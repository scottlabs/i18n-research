
<div class="search-results">
  <ul class="search-result-list">
    {{~it :result:index}}
      <li class="search-result {{?index === 0}}active{{?}}" data-index="{{=index}}" data-key="{{!result.key}}">
        <h5 class="search-result-key">{{!result.key}}</h5>
        <h6 class="search-result-value">{{!result.value}}</h6>
      </li>
    {{~}}
  </ul>
</div>
